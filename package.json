{
  "name": "cra-template-awesome",
  "version": "0.1.0",
  "keywords": [
    "react",
    "create-react-app",
    "template"
  ],
  "description": "A custom template for the Create React Application.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/vicasas/cra-template-awesome.git"
  },
  "bugs": {
    "url": "https://github.com/vicasas/cra-template-awesome/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "build:development": "env-cmd -f .env.development npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "node_modules/.bin/eslint \"**/*.+(js|jsx)\"",
    "lint:fix": "node_modules/.bin/eslint --fix \"**/*.+(js|jsx)\"",
    "pretty": "node_modules/.bin/prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "npm run lint:fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "npm run pretty",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4"
  }
}
